
public class TaskTwo {

	public static void main(String[] args) {
		
		int firstNumber = 6;
		int secondNumber = 7;
		int total = Calculator.add(firstNumber, secondNumber);
		String outputMessage = "The sum of " + 
		firstNumber + " and " + secondNumber 
		+ " is " + total;
		System.out.println(outputMessage);
	}
	
	
}



public class Calculator {

	public static void main(String[] args)
	
	int x = 5;
	int y = 10;
	
    public static int add(firstNumber, secondNumber)
    {
        int result = firstNumber + secondNumber;
        return result;
    }
    public static int subtract(firstNumber, secondNumber)
    {
        int result = firstNumber-secondNumber;
        return result;
    }
    public static int mult(firstNumber, secondNumber)
    {
        int result = firstNumber*secondNumber;
        return result;
    }
    public static int div(firstNumber, secondNumber)
    {
        int result = firstNumber/secondNumber;
        return result;
    }


	public int subtract(int firstNumber, int secondNumber) {

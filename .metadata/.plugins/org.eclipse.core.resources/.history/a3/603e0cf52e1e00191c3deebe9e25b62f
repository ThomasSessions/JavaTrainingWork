
public class TaskTwo {
	int firstNumber = 0;
	int secondNumber = 0;
	public static void number(int firstNumber, int secondNumber)
	{this.firstNumber = firstNumber; this.secondNumber = secondNumber;}
	
	public void completeTask() {
		
		//int firstNumber = Main.firstNumber;
		// int secondNumber = Main.secondNumber;
		
		
		int total = Calculations.add(firstNumber, secondNumber);
		String outputMessage = "The sum of " + 
		firstNumber + " and " + secondNumber 
		+ " is " + total;
		System.out.println(outputMessage);
	
	
	
		total = Calculations.multi(firstNumber, secondNumber);
		String outputMessage1 = "The sum of " + 
		firstNumber + " and " + secondNumber 
		+ " is " + total;
		System.out.println(outputMessage1);
	}
}


package People;

public class Person {
		private String name;
		private int age;
		private String jobTitle;
		
		public String getName() {
			return name;
		}
		
		public void setName(String name) {
			this.name = name;
		}
		
		public int getAge() {
			return age;
		}
		
		public void setAge(int age) {
			this.age = age;
		}
		public int getjobTitle() {
			return jobTitle;
		}
		
		public void setjobTitle(String jobTitle) {
			this.jobTitle = jobTitle;
		}
	}
